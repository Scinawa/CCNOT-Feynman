Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "findCompBasis", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"findCompBasis", "[", "s_", "]"}], ":=", " ", 
  RowBox[{
   RowBox[{"findCompBasis", "[", "s", "]"}], " ", "=", " ", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "range", ",", "appo", ",", "lista", ",", "prod", ",", "tabella"}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"range", "=", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"IntegerDigits", "[", 
           RowBox[{"#", ",", "2", ",", "s"}], "]"}], "&"}], ",", 
         RowBox[{"Range", "[", 
          RowBox[{"0", ",", 
           RowBox[{
            SuperscriptBox["2", "s"], "-", "1"}]}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"appo", "=", 
       RowBox[{"Table", "[", " ", 
        RowBox[{
         RowBox[{"Reverse", "[", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"1", "-", 
                RowBox[{"range", "[", 
                 RowBox[{"[", 
                  RowBox[{"j", ",", "i"}], "]"}], "]"}]}], ")"}], "*", 
              "down"}], "+", 
             RowBox[{
              RowBox[{"range", "[", 
               RowBox[{"[", 
                RowBox[{"j", ",", "i"}], "]"}], "]"}], "*", "up"}]}], ",", 
            RowBox[{"{", 
