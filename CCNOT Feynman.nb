         RowBox[{"{", 
          RowBox[{"j", ",", "1", ",", 
           RowBox[{"Length", "[", "range", "]"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"tabella", "=", 
       RowBox[{"Table", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"Clear", "[", "lista", "]"}], ";", "\[IndentingNewLine]", 
          RowBox[{"lista", "=", 
           RowBox[{"appo", "[", 
            RowBox[{"[", "k", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"prod", " ", "=", " ", 
           RowBox[{"lista", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"For", "[", 
           RowBox[{
            RowBox[{"i", "=", "2"}], ",", 
            RowBox[{"i", "\[LessEqual]", 
             RowBox[{"Length", "[", "lista", "]"}]}], ",", 
            RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
            RowBox[{"prod", "=", 
             RowBox[{"Flatten", "[", 
              RowBox[{"KroneckerProduct", "[", 
               RowBox[{"prod", ",", 
                RowBox[{"lista", "[", 
                 RowBox[{"[", "i", "]"}], "]"}]}], "]"}], "]"}]}]}], 
           "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", "prod"}],
          ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "1", ",", 
           RowBox[{"Length", "[", "appo", "]"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
